{
    "manifest_version": 3,
    "name": "ProcrastinateIdea",
    "version": "1.0.0",
    "content_scripts": [
        {
        "matches": ["<all_urls>"],
        "js": ["Content.js","popup.js"]
        }
    ],
    "action": {
        "default_popup": "popup.html",
        "default_title": "SumPopup"
    }
}